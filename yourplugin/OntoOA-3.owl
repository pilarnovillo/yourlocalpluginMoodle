                    <rdf:Description rdf:about="http://www.semanticweb.org/valer/ontologies/OntoOA#ComponenteInterno"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/valer/ontologies/OntoOA#actividadSeComponeDe"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/valer/ontologies/OntoOA#Subactividad"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/valer/ontologies/OntoOA#actividadSeComponeDe"/>
                        <owl:allValuesFrom rdf:resource="http://www.semanticweb.org/valer/ontologies/OntoOA#Subactividad"/>
